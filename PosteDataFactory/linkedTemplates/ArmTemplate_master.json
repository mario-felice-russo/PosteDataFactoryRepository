{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "PosteDataFactory"
		},
		"ls_blob_population_compressed_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_population_compressed'"
		},
		"ls_blob_population_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_blob_population_datalake'"
		},
		"ls_db_sql_azure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_db_sql_azure'"
		},
		"ls_dl_population_filled_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dl_population_filled'"
		},
		"ls_blob_population_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://postedatalake.dfs.core.windows.net"
		},
		"ls_dl_population_filled_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://postedatalake.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "PosteDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blob_population_compressed_connectionString": {
						"value": "[parameters('ls_blob_population_compressed_connectionString')]"
					},
					"ls_blob_population_datalake_accountKey": {
						"value": "[parameters('ls_blob_population_datalake_accountKey')]"
					},
					"ls_db_sql_azure_connectionString": {
						"value": "[parameters('ls_db_sql_azure_connectionString')]"
					},
					"ls_dl_population_filled_accountKey": {
						"value": "[parameters('ls_dl_population_filled_accountKey')]"
					},
					"ls_blob_population_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_blob_population_datalake_properties_typeProperties_url')]"
					},
					"ls_dl_population_filled_properties_typeProperties_url": {
						"value": "[parameters('ls_dl_population_filled_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}